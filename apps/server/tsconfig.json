{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "es2015",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "sourceMap": true,
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}